{
    "name": "PHP code with find the geolocation using google api.",
    "type": "library",
    "description": "Geolocation is a PHP HTTP client library using Google and Guzzle.",
    "keywords": [
        "Geolocation",
        "http",
        "rest",
        "web service",
        "curl",
        "client",
        "HTTP client",
		"PHP",
		"Traits",
		"Google"
    ],
    "homepage": "https://github.com/sathishfreaks5",
    "license": "MIT",
    "authors": [
        {
            "name": "sathish kumar",
            "email": "sathishfreaks5@gmail.com",
            "homepage": "https://github.com/sathishfreaks5"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "guzzlehttp/promises": "^1.0",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0",
        "psr/log": "^1.1"
    },
    "suggest": {
        "psr/log": "Required for using the Log middleware"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.3-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "GuzzleHttp\\": "src/"
        },
        "files": [
            "src/functions_include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GuzzleHttp\\Tests\\": "tests/"
        }
    }
}